<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <Button
        android:id="@+id/send_usual_notification"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send usual notification"/>
    <Button
        android:id="@+id/send_notification_with_action"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send notification with action"/>
    <Button
        android:id="@+id/send_notification_wearable_only"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send notification wearable-only action"/>
    <Button
        android:id="@+id/send_notification_with_big_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send notification with big text"/>
    <Button
        android:id="@+id/send_notification_with_vice_input"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send notification with vice input"/>
    <Button
        android:id="@+id/send_notification_with_pages"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send notification with pages"/>
</LinearLayout>